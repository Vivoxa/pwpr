<div id="main-container" class="container">
  <div class="card">
    <h3 class="card-header">
      <center>Materials</center>
    </h3>
    <div class="card-block">
      <p class="card-text">
        <%= form_for @material_detail, url: material_detail_path(@user), html: { method: :put } do %>
        <% end %>
      </p>
    </div>
    <div class="card-footer">
      <%= link_to 'Back to Member', @business , class: ['btn btn-sm btn-outline-primary', 'show-btn-lg'] %>
    </div>
  </div>
</div>
